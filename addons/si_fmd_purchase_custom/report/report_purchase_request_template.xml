<odoo>
    <template id="report_purchase_request_template">
        <t t-call="web.basic_layout">
            <t t-set="doc" t-value="docs"/>
            <div class="page">
                <h2>Purchase Order Attributes</h2>
                <t t-if="data['attributes']">
                    <table class="table table-sm table-hover">
                        <thead>
                            <tr>
                                <th>Attribute</th>
                                <th>Value</th>
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="data['attributes'].items()" t-as="attr">
                                <tr>
                                    <td><t t-esc="attr[0]"/></td>
                                    <td><t t-esc="attr[1]"/></td>
                                </tr>
                            </t>
                        </tbody>
                    </table>
                </t>
                <t t-else="">
                    <p>No attributes available.</p>
                </t>
            </div>
        </t>
    </template>
</odoo>